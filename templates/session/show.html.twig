

{% block title %}Session{% endblock %}

{% block body %}
    <h1>Session</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ session.id }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ session.dateDebut ? session.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ session.dateFin ? session.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateDebue</th>
                <td>{{ session.dateDebue ? session.dateDebue|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateFine</th>
                <td>{{ session.dateFine ? session.dateFine|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>NomFormation</th>
                <td>{{ session.nomFormation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_Fourmateur') }}">back to list</a>

    <a href="{{ path('app_session_edit', {'id': session.id}) }}">edit</a>

    {{ include('session/_delete_form.html.twig') }}
{% endblock %}
